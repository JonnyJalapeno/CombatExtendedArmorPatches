<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[RH2] Rimmu-Nation² - Clothing</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]/statBases</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]</xpath>
            <value>
              <statBases />
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]/statBases</xpath>
          <value>
            <statBases>
              <MaxHitPoints>100</MaxHitPoints>
              <Flammability>1</Flammability>
              <DeteriorationRate>2</DeteriorationRate>
              <Beauty>-3</Beauty>
              <WorkToMake>4000</WorkToMake>
              <Mass>0.08</Mass>
              <StuffEffectMultiplierArmor>1.5</StuffEffectMultiplierArmor>
              <StuffEffectMultiplierInsulation_Cold>0.05</StuffEffectMultiplierInsulation_Cold>
              <StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
              <EquipDelay>0.75</EquipDelay>
              <Bulk>3</Bulk>
              <WornBulk>0.1</WornBulk>
              <ArmorRating_Sharp>2</ArmorRating_Sharp>
              <ArmorRating_Blunt>2</ArmorRating_Blunt>
            </statBases>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]</xpath>
          <value>
            <li Class="CombatExtended.PartialArmorExt">
              <stats />
            </li>
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]/equippedStatOffsets</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]</xpath>
            <value>
              <equippedStatOffsets />
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]/equippedStatOffsets</xpath>
          <value>
            <equippedStatOffsets>
              <ShootingAccuracyPawn>-0.05</ShootingAccuracyPawn>
            </equippedStatOffsets>
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]/apparel</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RNApparel_EyePros_8X10BallisticGoggles"]</xpath>
            <value>
              <apparel />
            </value>
          </nomatch>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>