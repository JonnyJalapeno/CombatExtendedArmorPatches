<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[RH2] Rimmu-Nation² - Clothing</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]/statBases</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]</xpath>
            <value>
              <statBases />
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]/statBases</xpath>
          <value>
            <statBases>
              <MaxHitPoints>75</MaxHitPoints>
              <Flammability>0.6</Flammability>
              <DeteriorationRate>2</DeteriorationRate>
              <Beauty>-3</Beauty>
              <WorkToMake>10000</WorkToMake>
              <Mass>4.5</Mass>
              <Bulk>15</Bulk>
              <WornBulk>5</WornBulk>
              <ArmorRating_Sharp>14</ArmorRating_Sharp>
              <ArmorRating_Blunt>67</ArmorRating_Blunt>
              <ArmorRating_Heat>0.27</ArmorRating_Heat>
              <Insulation_Cold>1</Insulation_Cold>
              <EquipDelay>4.5</EquipDelay>
            </statBases>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]</xpath>
          <value>
            <li Class="CombatExtended.PartialArmorExt">
              <stats>
                <li>
                  <ArmorRating_Sharp>0.429</ArmorRating_Sharp>
