<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[JDS] EFT Apparel</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]/statBases</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]</xpath>
            <value>
              <statBases />
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]/statBases</xpath>
          <value>
            <statBases>
              <MaxHitPoints>100</MaxHitPoints>
              <Flammability>1</Flammability>
              <DeteriorationRate>2</DeteriorationRate>
              <Beauty>-3</Beauty>
              <MarketValue>274</MarketValue>
              <Mass>1.2</Mass>
              <ArmorRating_Sharp>0.25</ArmorRating_Sharp>
              <ArmorRating_Blunt>1</ArmorRating_Blunt>
              <ArmorRating_Heat>0.42</ArmorRating_Heat>
              <Insulation_Cold>0.5</Insulation_Cold>
              <Insulation_Heat>0.02</Insulation_Heat>
              <EquipDelay>1</EquipDelay>
              <WorkToMake>3000</WorkToMake>
            </statBases>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]</xpath>
          <value>
            <li Class="CombatExtended.PartialArmorExt">
              <stats />
            </li>
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]/equippedStatOffsets</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]</xpath>
            <value>
              <equippedStatOffsets />
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]/equippedStatOffsets</xpath>
          <value>
            <equippedStatOffsets />
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]/apparel</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="JDSTarkov_Headgear_Soft_Tank_Crew"]</xpath>
            <value>
              <apparel />
            </value>
          </nomatch>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>