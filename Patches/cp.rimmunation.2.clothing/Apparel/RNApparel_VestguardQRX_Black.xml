                    <li>Pelvis</li>
                    <li>Clavicle</li>
                    <li>Waist</li>
                    <li>Intestines</li>
                    <li>Shoulder</li>
                  </parts>
                </li>
                <li>
                  <ArmorRating_Blunt>0.429</ArmorRating_Blunt>
                  <parts>
                    <li>Pelvis</li>
                    <li>Clavicle</li>
                    <li>Waist</li>
                    <li>Intestines</li>
                    <li>Shoulder</li>
                  </parts>
                </li>
              </stats>
            </li>
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]/equippedStatOffsets</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]</xpath>
            <value>
              <equippedStatOffsets />
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationReplace">
          <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]/equippedStatOffsets</xpath>
          <value>
            <equippedStatOffsets />
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]/apparel</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="RNApparel_VestguardQRX_Black"]</xpath>
            <value>
              <apparel />
            </value>
          </nomatch>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>