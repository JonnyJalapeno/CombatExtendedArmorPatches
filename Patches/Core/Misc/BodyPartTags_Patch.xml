<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <!-- Add vital tag organ -->
    <Operation Class="PatchOperationAdd">
      <xpath>/Defs</xpath>
      <value>
        <BodyPartTagDef>
          <defName>isVital</defName>
          <vital>true</vital>
        </BodyPartTagDef>
      </value>
    </Operation>
    
</Patch>